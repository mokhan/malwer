<div class="row">
<div class="small-12 columns">
  <h1>Login</h1>

  <%= form_for @user, url: sessions_path do |f| %>
    <div class="field">
      <%= label_tag :username %><br>
      <%= text_field_tag :username %>
    </div>
    <div class="field">
      <%= label_tag :password %><br>
      <%= password_field_tag :password %>
    </div>
    <div class="actions">
      <%= submit_tag %>
    </div>
  <% end %>
</div>
</div>
